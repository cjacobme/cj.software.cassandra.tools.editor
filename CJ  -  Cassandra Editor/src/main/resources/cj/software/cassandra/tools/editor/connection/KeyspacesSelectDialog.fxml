<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane minHeight="300.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="cj.software.cassandra.tools.editor.connection.KeyspacesSelectDialogController">
   <children>
      <ListView fx:id="keyspacesList" prefHeight="200.0" prefWidth="200.0" AnchorPane.leftAnchor="5.0" AnchorPane.rightAnchor="5.0" AnchorPane.topAnchor="5.0" />
      <ButtonBar prefHeight="40.0" prefWidth="200.0" AnchorPane.bottomAnchor="10.0" AnchorPane.rightAnchor="10.0">
        <buttons>
            <Button mnemonicParsing="false" onAction="#reloadKeyspaces" text="Reload" />
            <Button cancelButton="true" mnemonicParsing="false" onAction="#handleCancelBtn" text="Cancel" />
          <Button defaultButton="true" mnemonicParsing="false" onAction="#handleOk" text="OK" />
        </buttons>
      </ButtonBar>
   </children>
</AnchorPane>
